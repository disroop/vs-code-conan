{
	"license": "MIT",
	"name": "conan",
	"displayName": "conan",
	"description": "Conan C/C++ package manager integration for VS-Code",
	"version": "0.7.1",
	"publisher": "disroop",
	"icon": "images/disroop-icon.png",
	"preview": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/disroop/vs-code-conan.git"
	},
	"engines": {
		"vscode": "^1.48.2"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/conanfile.py",
		"workspaceContains:**/.vscode/conan-settings.json"
	],
	"main": "./out/src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vs-code-conan.profilePick",
				"title": "Conan: Pick profile "
			},
			{
				"command": "vs-code-conan.install",
				"title": "Conan: Install packages"
			},
			{
				"command": "vs-code-conan.build",
				"title": "Conan: Build"
			},
			{
				"command": "vs-code-conan.create",
				"title": "Conan: Create package"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p . ",
		"lint": "eslint src --ext .ts",
		"watch": "tsc -watch -p ./",
		"test": "nyc --reporter=lcovonly --reporter=text mocha --require ts-node/register -R @danmasta/mocha-sonar  test/unit/**/*.ts",
		"integration-test": "node ./out/test/integration/runTest.js"
	},
	"devDependencies": {
		"@danmasta/mocha-sonar": "^1.0.1",
		"@types/chai": "^4.2.22",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^9.0.0",
		"@types/node": "^16.10.4",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^5.0.0",
		"@typescript-eslint/parser": "^5.0.0",
		"chai": "^4.3.4",
		"coveralls": "^3.1.1",
		"eslint": "^8.0.0",
		"eslint-config-prettier": "^8.3.0",
		"glob": "^7.1.6",
		"mocha": "^9.1.2",
		"mocha-lcov-reporter": "^1.3.0",
		"nyc": "^15.1.0",
		"prettier": "2.2.1",
		"reflect-metadata": "^0.1.13",
		"ts-node": "^10.2.1",
		"typescript": "^4.4.3",
		"typescript-eslint": "^0.0.1-alpha.0",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"queue-typescript": "^1.0.1",
		"tree-kill": "^1.2.2",
		"tsyringe": "^4.6.0",
		"vsce": "^2.0.0"
	}
}
